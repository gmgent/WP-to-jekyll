---
---

<rss version="2.0">
<channel>
  <title>RockstarAlley by Krister Axel</title>
  <link>http://kristeraxel.com/</link>
  <description>new songs every month</description>
  <language>en-us</language>
  <pubDate>{{ site.time | date_to_long_string }}</pubDate>
    {% for post in site.categories.rockstaralley | limit: 5 %}
      <item>
        <title>{{ post.title }}</title>
        <link>{{ post.url }}</link>
        <description>{{ post.content | escape }}</description>
        <pubDate>{{ post.date }}</pubDate>
      </item>
    {% endfor %}
</channel>
</rss>